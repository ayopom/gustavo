{
  "name": "gustavo",
  "version": "1.0.0",
  "description": "A (mostly) headless blogging platform built atop Nuxt & Gist.",
  "author": "Brian Gonzalez <me@briangonzalez.org>",
  "scripts": {
    "dev": "backpack dev",
    "start": "(NODE_ENV=production nuxt build) && (NODE_ENV=production backpack build) && (NODE_ENV=production node build/main.js)",
    "precommit": "npm run lint",
    "lint": "eslint --ext .html,.js,.vue --ignore-path .gitignore .",
    "docker-publish": "docker build -t eggplanet/gustavo . && docker push eggplanet/gustavo"
  },
  "dependencies": {
    "@nuxtjs/cssnano": "^0.1.1",
    "@nuxtjs/google-analytics": "^0.2.0",
    "@nuxtjs/manifest": "latest",
    "@nuxtjs/offline": "latest",
    "@nuxtjs/optimize": "latest",
    "axios": "^0.16.1",
    "babel-cli": "^6.24.1",
    "babel-polyfill": "^6.23.0",
    "backpack-core": "^0.3.0",
    "compression": "^1.6.2",
    "cross-env": "^5.0.0",
    "express": "^4.15.3",
    "express-request-proxy": "^2.0.0",
    "html-element": "^2.2.0",
    "lodash.uniqby": "^4.7.0",
    "morgan": "^1.8.2",
    "node-sass": "^4.5.3",
    "normalize.css": "^7.0.0",
    "nuxt": "^1.0.0-alpha.3",
    "sass-loader": "^6.0.5",
    "showdown": "^1.7.1",
    "source-map-support": "^0.4.15",
    "vue-highlightjs": "^1.3.3",
    "vue-moment": "^2.0.2",
    "xmldom": "^0.1.27"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-preset-env": "^1.5.1",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-html": "^2.0.3",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-node": "^5.0.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-vue": "^2.0.1",
    "nodemon": "^1.11.0"
  }
}
